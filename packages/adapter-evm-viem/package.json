{
  "name": "@web3-hooks/adapter-evm-viem",
  "version": "1.0.0",
  "type": "module",
  "private": false,
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  },
  "packageManager": "pnpm@9.12.3",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "peerDependencies": {
    "@web3-hooks/core": "0.2.0",
    "viem": "^2.12.0"
  },
  "devDependencies": {
    "@web3-hooks/core": "workspace:*",
    "tsup": "^8.3.0",
    "typescript": "^5.9.3",
    "viem": "^2.12.0"
  },
  "scripts": {
    "build": "tsup src/index.ts --dts --format esm,cjs",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "prepublishOnly": "pnpm run build",
    "lint": "eslint .",
    "test": "vitest run"
  }
}
